#include <stdio.h>

int main()

{

    int a[100],size,i,j,temp=0;

    printf("\n\n Enter size of array::");

    scanf("%d",&size);

    printf("\n\n Enter %d array elements::",n);

    for(i=0;i<size;i++)

    {

       scanf("%d",&a[i]);

    }

    printf("\n\n Sorted array::");

    for(i=0;i<size-1;i++)

    {

        for(j=0;j<size-i-1;j++)

        {

            if(a[j]>a[j+1])

            {

               temp=a[j];

               a[j]=a[j+1];

               a[j+1]=temp;

            }

         }

     }

     for(i=0;i<size;i++)

     {

         printf("\t%d",a[i]);

     }

return 0;

}
